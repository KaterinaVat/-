
#include <iostream>
#include <string>
int main() {
	int count = 0;
	std::cout << "Give count of strings ";
	std::cin >> count;
	while (count--) {
		std::string a;
		std::cout << a;
		for (int i = 0; i < size(a)-3; i++) {
			if (((a[i] != a[i + 1]) && (a[i] != a[i + 2]) && (a[i + 1] != a[i + 2])) && (a[i] != '.')) {
				std::cout << "YES" << std::endl;

			}
			else {
				std::cout << "NO" << std::endl;
			}
		}
	}
}
